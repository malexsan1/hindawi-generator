# run from AWS CLI

# use the below command to create the Elastic Beanstalk environment

aws elasticbeanstalk create-environment \
  --cname-prefix <%= enviromentElasticbeanstalk%>-service-<%= serviceName%>
  --application-name Screening \
  --template-name generic-app \
  --version-label <%= enviromentElasticbeanstalk%>-service-<%= serviceName%> \
  --environment-name <%= enviromentElasticbeanstalk%>-service-<%= serviceName%>



# use the below command to list the enviroment variables of a targeted AWS enviroment
# replace name_of_enviroment with your target

eb printenv name_of_enviroment


# use the below command to upload enviroment variables to a targeted AWS enviroment
# replace name_of_enviroment with your target
# add ENV_VARIABLE_NAME1,2,3,etc and ENV_VARIABLE_VALUE1,2,3,etc with the ones you need

eb setenv -e name_of_enviroment \
ENV_VARIABLE_NAME1=ENV_VARIABLE_VALUE1 \
ENV_VARIABLE_NAME1=ENV_VARIABLE_VALUE2 \
ENV_VARIABLE_NAME1=ENV_VARIABLE_VALUE3
